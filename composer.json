{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel-lang/locale-list": "^1.4",
        "laravel-lang/locales": "^2.9",
        "laravel/framework": "^v11.15.0",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.5",
        "munafio/chatify": "^1.6.2",
        "nesbot/carbon": "^3.6",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-permission": "^6.9",
        "tightenco/ziggy": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/common": "^6.4",
        "laravel-lang/http-statuses": "^3.8",
        "laravel-lang/lang": "^15.5",
        "laravel-lang/publisher": "^16.4",
        "laravel/breeze": "^2.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.30",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
